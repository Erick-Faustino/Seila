import java.util.ArrayList;
import java.util.List;

class Agenda {
    private String profissional;
    private List<Horario> horariosDisponiveis;

    public Agenda(String profissional) {
        this.profissional = profissional;
        this.horariosDisponiveis = new ArrayList<>();
    }

    public void adicionarHorarioDisponivel(Horario horario) {
        horariosDisponiveis.add(horario);
    }

    public void removerHorarioDisponivel(Horario horario) {
        horariosDisponiveis.remove(horario);
    }

    public List<Horario> getHorariosDisponiveis() {
        return horariosDisponiveis;
    }

    public String getProfissional() {
        return profissional;
    }
}

class Horario {
    private String data;
    private String hora;

    public Horario(String data, String hora) {
        this.data = data;
        this.hora = hora;
    }

    public String getData() {
        return data;
    }

    public String getHora() {
        return hora;
    }
}

public class Main {
    public static void main(String[] args) {
        Agenda agendaDrSmith = new Agenda("Dr. Smith");

        Horario horario1 = new Horario("2023-10-15", "09:00");
        Horario horario2 = new Horario("2023-10-15", "10:00");

        agendaDrSmith.adicionarHorarioDisponivel(horario1);
        agendaDrSmith.adicionarHorarioDisponivel(horario2);

        System.out.println("Agenda do Dr. Smith:");
        for (Horario horario : agendaDrSmith.getHorariosDisponiveis()) {
            System.out.println(horario.getData() + " Ã s " + horario.getHora());
        }
    }
}
