import java.util.ArrayList;
import java.util.List;

class Agenda {
    private String profissional;
    private List<Horario> horariosDisponiveis;
    private List<Horario> horariosBloqueados;

    public Agenda(String profissional) {
        this.profissional = profissional;
        this.horariosDisponiveis = new ArrayList<>();
        this.horariosBloqueados = new ArrayList<>();
    }

    public void adicionarHorarioDisponivel(Horario horario) {
        horariosDisponiveis.add(horario);
    }

    public void bloquearHorario(Horario horario) {
        if (horariosDisponiveis.contains(horario)) {
            horariosDisponiveis.remove(horario);
            horariosBloqueados.add(horario);
        }
    }

    public void desbloquearHorario(Horario horario) {
        if (horariosBloqueados.contains(horario)) {
            horariosBloqueados.remove(horario);
            horariosDisponiveis.add(horario);
        }
    }

    public List<Horario> getHorariosDisponiveis() {
        return horariosDisponiveis;
    }

    public List<Horario> getHorariosBloqueados() {
        return horariosBloqueados;
    }

    public String getProfissional() {
        return profissional;
    }
}

class Horario {
    private String data;
    private String hora;

    public Horario(String data, String hora) {
        this.data = data;
        this.hora = hora;
    }

    public String getData() {
        return data;
    }

    public String getHora() {
        return hora;
    }
}

public class Main {
    public static void main(String[] args) {
        Agenda agendaDrSmith = new Agenda("Dr. Smith");

        Horario horario1 = new Horario("2023-10-15", "09:00");
        Horario horario2 = new Horario("2023-10-15", "10:00");

        agendaDrSmith.adicionarHorarioDisponivel(horario1);
        agendaDrSmith.adicionarHorarioDisponivel(horario2);

        System.out.println("Agenda do Dr. Smith (Disponíveis):");
        for (Horario horario : agendaDrSmith.getHorariosDisponiveis()) {
            System.out.println(horario.getData() + " às " + horario.getHora());
        }

        // Bloquear horário
        agendaDrSmith.bloquearHorario(horario1);

        System.out.println("\nAgenda do Dr. Smith (Bloqueados):");
        for (Horario horario : agendaDrSmith.getHorariosBloqueados()) {
            System.out.println(horario.getData() + " às " + horario.getHora());
        }

        // Desbloquear horário
        agendaDrSmith.desbloquearHorario(horario1);

        System.out.println("\nAgenda do Dr. Smith (Após Desbloqueio):");
        for (Horario horario : agendaDrSmith.getHorariosDisponiveis()) {
            System.out.println(horario.getData() + " às " + horario.getHora());
        }
    }
}
